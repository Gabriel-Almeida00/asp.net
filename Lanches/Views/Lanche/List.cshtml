@model IEnumerable<Lanches.Models.Lanche>

<div>
    <h2>@ViewData["Titulo"]</h2>
</div>
<h2>@ViewData["Data"]</h2>
<hr />
<h3>@ViewBag.Total @ViewBag.TotalLanches</h3>
<hr />

<h3>@TempData["Nome"]; </h3>

@foreach (var lanche in Model)
{
    <div>
        <h4>Lanche : @lanche.Nome</h4>
        <p><img src="@lanche.ImagemUrl"></img></p>
        <p>Preço : @lanche.Preco.ToString("c")</p>
    </div>
}
